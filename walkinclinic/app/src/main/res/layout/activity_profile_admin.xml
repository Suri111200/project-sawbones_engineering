<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/background3"
    android:layout_height="match_parent"
    tools:context=".ProfileAdmin">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="8dp"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/nameAText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="-90dp"
            android:layout_marginTop="290dp"
            android:fontFamily="@font/clanpro_news"
            android:text="TextView"
            android:textSize="38dp" />

        <TextView
            android:id="@+id/typeAText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="-143dp"
            android:layout_marginTop="16dp"
            android:fontFamily="@font/clanpro_news"
            android:text="TextView" />

        <TextView
            android:id="@+id/emailAText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="-143dp"
            android:layout_marginTop="16dp"
            android:fontFamily="@font/clanpro_news"
            android:text="TextView" />

        <Button
            android:id="@+id/serviceB"
            android:layout_width="147dp"
            android:layout_height="43dp"
            android:background="@drawable/button_border2"
            android:text="Services"
            android:textColor="#FFFFFF"
            android:textColorHighlight="#00F8F5F5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.844"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.954"
            android:layout_marginLeft="-90dp"
            android:layout_marginTop="250dp" />

        <Button
            android:id="@+id/usersB"
            android:layout_width="145dp"
            android:layout_height="44dp"
            android:background="@drawable/button_border"
            android:text="Users"
            android:textColor="#000000"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.139"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.956"
            android:layout_marginLeft="91dp"
            android:layout_marginTop="-43dp" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>